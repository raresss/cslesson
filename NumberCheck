def SearchNumber(input):
	age = [1,2,3,4,5,6,7,8]
	if input == age[0]:
		print("The number is in the list")
	if input == age[1]:
		print("The number is in the list")
	if input == age[2]:
		print("The number is in the list")
	if input == age[3]:
		print("The number is in the list")
	if input == age[4]:
		print("The number is in the list")
	if input == age[5]:
		print("The number is in the list")
	if input == age[6]:
		print("The number is in the list")
	if input == age[7]:
		print("The number is in the list")
	if input!= age[0]:
		print("The number is NOT in the list")
	if input != age[1]:
		print("The number is NOT in the list")
	if input != age[2]:
		print("The number is NOT in the list")
	if input != age[3]:
		print("The number is NOT in the list")
	if input != age[4]:
		print("The number is NOT in the list")
	if input != age[5]:
		print("The number is NOT in the list")
	if input != age[6]:
		print("The number is NOT in the list")
	if input != age [7]:
		print("The number is NOT in the list")
